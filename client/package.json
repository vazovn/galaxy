{
  "name": "galaxy-client",
  "version": "0.1.0",
  "description": "Galaxy client application build system",
  "keywords": [
    "galaxy"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/galaxyproject/galaxy.git"
  },
  "license": "AFL-3.0",
  "dependencies": {
    "@handsontable/vue": "^2.0.0-beta1",
    "amdi18n-loader": "^0.7.0",
    "axios": "^0.18.0",
    "backbone": "1.3",
    "bibtex-parse-js": "^0.0.24",
    "bootstrap": "^4.1.1",
    "bootstrap-tour": "https://github.com/sorich87/bootstrap-tour.git#6a1028fb562f9aa68c451f0901f8cfeb43cad140",
    "bootstrap-vue": "^2.0.0-rc.11",
    "d3": "3",
    "decode-uri-component": "^0.2.0",
    "font-awesome": "^4.7.0",
    "gulp-sass": "^4.0.1",
    "handsontable": "^2.0.0",
    "jquery": "2",
    "jquery-migrate": "~1.4",
    "jquery-mousewheel": "^3.1.13",
    "jquery-ui-bundle": "^1.12.1",
    "jquery.complexify": "^0.5.2",
    "jquery.cookie": "^1.4.1",
    "latex-parser": "^0.6.2",
    "latex-to-unicode-converter": "^0.5.2",
    "popper.js": "^1.14.3",
    "pyre-to-regexp": "^0.0.5",
    "raven-js": "^3.26.3",
    "requirejs": "2",
    "underscore": "^1.9.1",
    "vue": "2.5.16"
  },
  "scripts": {
    "watch": "gulp staging && yarn run save-build-hash && concurrently \"yarn run webpack-watch\" \"yarn run gulp watch\" \"yarn run style-watch\"",
    "build": "NODE_ENV=development gulp staging && concurrently \"yarn run style\" \"yarn run webpack\" \"yarn run gulp clean && yarn run gulp\" && yarn run save-build-hash",
    "build-production": "NODE_ENV=production gulp staging && concurrently \"yarn run style\" \"yarn run webpack-production\" \"yarn run gulp clean && yarn run gulp-production\" && yarn run save-build-hash",
    "build-production-maps": "NODE_ENV=production gulp staging && concurrently \"yarn run style\" \"yarn run webpack-production-maps\" \"yarn run gulp clean && yarn run gulp-production-maps\" && yarn run save-build-hash",
    "webpack": "webpack -d",
    "save-build-hash": "(git rev-parse HEAD 2>/dev/null || echo '') >../static/client_build_hash.txt",
    "webpack-watch": "webpack -d --watch",
    "webpack-production": "webpack -p",
    "webpack-production-maps": "GXY_BUILD_SOURCEMAPS=1 webpack -p",
    "style": "grunt && gulp style",
    "style-watch": "grunt && gulp watch-style",
    "test": "GALAXY_TEST_AS_SINGLE_PACK=true karma start --verbose --single-run --browsers PhantomJS karma.config.js",
    "test-watch": "GALAXY_TEST_AS_SINGLE_PACK=true karma start --verbose --browsers PhantomJS karma.config.js",
    "gulp": "gulp",
    "gulp-production": "NODE_ENV=production gulp",
    "gulp-production-maps": "GXY_BUILD_SOURCEMAPS=1 NODE_ENV=production gulp",
    "build-toolshed": "grunt --app=toolshed",
    "jshint": "jshint --exclude='galaxy/scripts/libs/**' galaxy/scripts/**/*.js",
    "build-charts": "webpack -p --config ../config/plugins/visualizations/charts/webpack.config.js",
    "build-scatterplot": "NODE_PATH=./node_modules webpack -p --config ../config/plugins/visualizations/scatterplot/webpack.config.js",
    "prettier": "concurrently \"yarn run prettier-style\" \"yarn run prettier-scripts\"",
    "prettier-scripts": "prettier --write --tab-width 4 --print-width 120 \"galaxy/scripts/{,!(libs)/**/}/{*.js,*.vue}\"",
    "prettier-style": "prettier --write --tab-width 4 --print-width 120 \"galaxy/style/scss/**/*.scss\"",
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-plugin-transform-vue-template": "^0.4.2",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "concurrently": "^4.0.1",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^1.1.11",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-spritesmith": "^4.7.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-beautify": "^2.0.1",
    "gulp-cached": "^1.1.1",
    "gulp-if": "^2.0.2",
    "gulp-plumber": "^1.2.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.0",
    "gulp-watch": "^5.0.0",
    "jshint": "^2.9.4",
    "karma": "^1.7.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-polyfill": "^1.0.0",
    "karma-qunit": "^1.2.1",
    "karma-webpack": "^2.0.6",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "prettier": "^1.13.6",
    "qunitjs": "^2.4.1",
    "sass-loader": "^7.0.3",
    "sinon": "^4.1.2",
    "vue-loader": "^15.2.4",
    "vue-styleguidist": "^1.6.1",
    "vue-template-compiler": "2.5.16",
    "webpack": "^3.10.0",
    "yaml-loader": "^0.5.0"
  }
}
