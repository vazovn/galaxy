<?xml version="1.0"?>
<tool name="Get files" id="nels_file_browser" tool_type="data_source" add_galaxy_url="False" force_history_refresh="True" version="0.0.1">
    <description>from NeLS storage</description>
    <command interpreter="python">nels_importer.py --galaxy_tool_data_dir ${GALAXY_DATA_INDEX_DIR} --json_parameter_file "${output}"</command>
    <inputs action="https://nels.bioinfo.no/welcome.xhtml" check_values="False" method="post">
        <display>go to NELS Data Manager </display>
        <param name="appCallbackUrl" type="baseurl" value="/tool_runner?tool_id=nels_file_browser&amp;runtool_btn=Execute" />
        <param name="appName" type="hidden" value="Galaxy" />
    </inputs>
    <uihints minwidth="800"/>
    <outputs>
        <data name="output" format="auto" />
    </outputs>
    <options sanitize="False" refresh="True"/>
</tool>
